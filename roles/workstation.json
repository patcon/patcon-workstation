{
  "name": "workstation",
  "default_attributes": {
    "authorization": {
      "sudo": {
        "passwordless": true
      }
    },
    "homesick": {
      "gem_version": "1.1.1"
    },
    "janus": {
      "users": [
        "patcon"
      ]
    },
    "mosh": {
      "install_type": "source",
      "url": "https://github.com/rinne/mosh/archive/ssh-agent-forwarding-ng.tar.gz",
      "version": "1.2.4-5e9bc4069fadb2a5d7cdb10286828fbdc4e0fdb5"
    },
    "packages-cookbook": [
      "colordiff",
      "tmux",
      "tree",
      "xclip"
    ],
    "users": [
      "patcon"
    ]
  },
  "run_list": [
    "recipe[apt]",
    "recipe[workstation::git-upgrade]",
    "recipe[chef-dk]",
    "recipe[mosh]",
    "recipe[fish-shell]",
    "recipe[user::data_bag]",
    "recipe[oh-my-zsh]",
    "recipe[python]",
    "recipe[sudo]",
    "recipe[pantry]",
    "recipe[workstation::hub]",
    "recipe[workstation::dotfiles]",
    "recipe[janus]",
    "recipe[patcon_android-sdk]",
    "role[hardening]"
  ]
}
